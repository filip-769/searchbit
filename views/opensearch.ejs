<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
    <Image>/favicon.ico</Image>
    <InputEncoding>UTF-8</InputEncoding>
    <ShortName>Searchbit</ShortName>
    <LongName>Searchbit</LongName>
    <Description>Searchbit opensearch.xml with <%= autocomplete %> autocomplete</Description>
    <Url rel="results" type="text/html" method="get" template="<%= origin %>/search?q={searchTerms}"/>
    <Url rel="suggestions" type="application/x-suggestions+json" template="<%= origin %>/search?q={searchTerms}&amp;f=json&amp;t=autocomplete&amp;e=<%= autocomplete %>"/>
    <Url type="application/opensearchdescription+xml" rel="self" template="<%= origin %>/opensearch.xml?ac=<%= autocomplete %>" />
</OpenSearchDescription>